<?xml version="1.0" encoding="utf-8"?>
<s:Module xmlns:fx="http://ns.adobe.com/mxml/2009" 
		  xmlns:s="library://ns.adobe.com/flex/spark" 
		  xmlns:mx="library://ns.adobe.com/flex/mx" 
		  xmlns:view="org.shell.view.*"
		  width="90%" 
		  height="20%">
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	
	
	<s:VGroup id="childVGroup"
			  horizontalAlign="center"
			  verticalAlign="middle"
			  width="100%"
			  height="100%">
		<s:Group width="100%"
				 height="100%">
			<s:filters>
				<s:DropShadowFilter color="#000000" strength="1" blurX="5" blurY="5" distance="2" angle="90"/>
			</s:filters>
			<!-- Fills the background with a rectangle-->
			<s:Rect height="100%" 
					width="100%" 
					id="colorCanvas">
				<s:stroke>
					<s:SolidColorStroke id="stroke" 
										color="#FFFFFF" 
										weight="2" 
										alpha="1"/>
				</s:stroke>
				<s:fill>
					<s:SolidColor id="fill"
								  color="#FFFFFF"
								  alpha="1"/>
				</s:fill>
			</s:Rect>
			
			<s:Label id="childLabel"
					 color="0x000000"
					 fontFamily="Arial"
					 fontLookup="embeddedCFF"
					 fontSize="12"
					 fontStyle="normal"
					 fontWeight="normal"
					 paddingBottom="0"
					 paddingLeft="0"
					 paddingRight="0"
					 paddingTop="0"
					 textAlign="center"
					 verticalAlign = "middle"
					 />
		</s:Group>
		
	</s:VGroup>
</s:Module>
